<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Regression" parallel="false" thread-count="4" configfailurepolicy="continue" preserve-order="true"
       verbose="1">

    <listeners>
        <listener class-name="com.morningstar.aws.project.setup.Setup"/>
    </listeners>
    <parameter name="environment" value="uat"/>
    <parameter name="testrun-id" value="270098"/>
    <parameter name="login-type" value="standard-login"/>
    <parameter name="login-id" value="QAInstid_2"/>

    <test name="HypotheticalWorkflow_Fees">
        <classes>
            <class name="com.morningstar.aws.project.tests.scenariobuilder.standardscenariobuilder.hypothetical.AdvisoryService_HoldingLevelAnnualFeeValidation"/>
            <class name="com.morningstar.aws.project.tests.scenariobuilder.standardscenariobuilder.hypothetical.AdvisoryService_Hypo_AnnualFeeDefault"/>
            <class name="com.morningstar.aws.project.tests.scenariobuilder.standardscenariobuilder.hypothetical.AdvisoryService_Hypo_PortfolioLevelAnnualFeeValidation"/>
            <class name="com.morningstar.aws.project.tests.scenariobuilder.standardscenariobuilder.hypothetical.AdvisoryService_HypotheticalWorkflow_FlatFee"/>
        </classes>
    </test>
    <test name="HypotheticalWorkflow_Fees-AdminLogin">
        <parameter name="login-type" value="admin-login"/>
        <parameter name="login-id" value="administrator"/>
        <classes>
            <class name="com.morningstar.aws.project.tests.scenariobuilder.standardscenariobuilder.hypothetical.AdvisoryService_Hypo_DynamicUpdateOfExistingMaxFee"/>
        </classes>
    </test>
    <test name="HypotheticalWorkflow_CreateMaxFee">
        <parameter name="login-id" value="QAInstid_1"/>
        <classes>
            <class name="com.morningstar.aws.project.tests.scenariobuilder.standardscenariobuilder.hypothetical.AdvisoryService_HypotheticalWorkflow_NewMaxFee_HoldingComparison"/>
        </classes>
    </test>
</suite>