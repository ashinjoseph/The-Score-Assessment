<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Regression:Wells_Customization" parallel="false" thread-count="4" configfailurepolicy="continue" preserve-order="true"
       verbose="1">

    <listeners>
        <listener class-name="com.morningstar.aws.project.setup.Setup"/>
    </listeners>
    <parameter name="environment" value="qa"/>
    <parameter name="testrun-id" value="316535"/>
    <parameter name="login-type" value="standard-login"/>

    <test name="CIT header verification On">
        <parameter name="re-login" value="false"/>
        <parameter name="login-id" value="QAInstid_2"/>
        <classes>
            <class name="com.morningstar.aws.project.tests.portfolio.portfoliobuilder.VerifyReportHeader"/>
        </classes>
    </test>
    <test name="CIT header verification Off">
        <parameter name="re-login" value="false"/>
        <parameter name="login-id" value="QAInstid_3"/>
        <classes>
            <class name="com.morningstar.aws.project.tests.portfolio.portfoliobuilder.VerifyReportHeader"/>
        </classes>
    </test>
    <test name="CIT header verification User Choice">
        <parameter name="re-login" value="false"/>
        <parameter name="login-id" value="QAInstid_3"/>
        <classes>
            <class name="com.morningstar.aws.project.tests.portfolio.portfoliobuilder.VerifyReportHeader"/>
        </classes>
    </test>
</suite>