<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Smoke Storm" verbose="1">

    <listeners>
        <listener class-name="com.morningstar.aws.project.setup.Setup"/>
    </listeners>
    <parameter name="environment" value="stg"/>
    <parameter name="testrun-id" value="313329"/>
    <parameter name="login-type" value="standard-login"/>

    <test name="ReportVerification_MaxSchedule_Advisory">
        <parameter name="login-id" value="QAInstid_2"/>
        <classes>
            <class name="com.morningstar.aws.project.tests.portfolio.portfoliobuilder.advisory.reports.VerifyWorkflowAndDisclosure"/>
            <class name="com.morningstar.aws.project.tests.portfolio.portfoliobuilder.advisory.reports.VerifyWorkflowAndDisclosure_MaxSchedule"/>
            <class name="com.morningstar.aws.project.tests.portfolio.portfolioreportbuilder.advisory.reports.VerifyWorkflowAndDisclosure"/>
        </classes>
    </test>
    <test name="ReportVerification_MaxSchedule_Advisory_ThreeReturns">
        <parameter name="login-id" value="TGROUP_1"/>
        <classes>
            <class name="com.morningstar.aws.project.tests.portfolio.portfoliobuilder.advisory.reports.VerifyWorkflowAndDisclosure_MaxSchedule_ThreeReturns"/>
            <class name="com.morningstar.aws.project.tests.portfolio.portfolioreportbuilder.advisory.reports.VerifyWorkflowAndDisclosure_ThreeReturns"/>
        </classes>
    </test>
    <test name="ReportVerification_MaxSchedule_Brokerage">
        <parameter name="login-id" value="QAInstid_2"/>
        <classes>
            <class name="com.morningstar.aws.project.tests.portfolio.portfoliobuilder.brokerage.reports.VerifyWorkflowAndDisclosure"/>
            <class name="com.morningstar.aws.project.tests.portfolio.portfolioreportbuilder.brokerage.reports.VerifyWorkflowAndDisclosure"/>
        </classes>
